{
    "name": "jamav/htdocs",
    "require": {
        "slim/slim": "4.*",
        "slim/psr7": "^1.3",
        "illuminate/database": "^7.29",
        "illuminate/events": "^7.29",
        "google/apiclient": "^2.0",
        "phpseclib/phpseclib": "^2.0",
        "myclabs/php-enum": "^1.7",
        "doctrine/inflector": "1.4.0",
        "google/cloud-pubsub": "^1.30",
        "twilio/sdk": "^6.20",
        "phpmailer/phpmailer": "^6.2"
    },
    "scripts": {
      
    },
    "extra": {
        "google/apiclient-services": [
            "Drive",
            "YouTube"
        ]
    },
    "autoload": {
        "psr-4": {
            "Models\\": "src/Models",
            "Controllers\\": "src/Controllers",
            "Components\\": "src/Components",
            "Middlewares\\": "src/Middlewares",
            "Config\\": "Config",
            "Enum\\": "src/Enum",
            "Servicios\\": "src/Servicios",
            "ServicioSMS\\": "src/ServicioSMS"
        }
    }
}
